# OK
# catalytic sythesis, dissociation and deletion
begin model
begin parameters

end parameters
begin species
	A(b)  0
	B(a)  200
end species
begin reaction rules
	B(a)           ->  B(a!1).A(b!1)    10
	B(a!1).A(b!1)  ->  B(a) + A(b)      10
	A(b)           ->  0                1
end reaction rules
begin observables
    Molecules Dimer B(a!1).A(b!1)
    Molecules Free  A(b)
    Molecules Total A
end observables
end model

generate_network({overwrite=>1})
simulate_ode({suffix=>"ode",t_end=>5,n_steps=>100})
resetConcentrations()
simulate_ssa({suffix=>"ssa",t_end=>5,n_steps=>100})
resetConcentrations()
writeXML()

